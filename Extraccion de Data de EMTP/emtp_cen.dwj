        data_AllSG += ",,T_dp," + oDevSG.Tdp + ",s" + ",\n";
        data_AllSG += ",,T_dpp," + oDevSG.Tdpp + ",s" + ",\n";
        data_AllSG += ",,X_qp," + oDevSG.Xqp + ",pu" + ",\n";
        data_AllSG += ",,X_qpp," + oDevSG.Xqpp + ",pu" + ",\n";
