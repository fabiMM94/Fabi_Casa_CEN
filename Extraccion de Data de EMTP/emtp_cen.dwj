      var devLines = all_lines_PI[j];
      var a = devLines.getAttribute("ModelData").split("\n");
      var R = a[0].split(/\s/);
      var L = a[1].split(/\s/);
      var C = a[2].split(/\s/);
      data_lines +=
        devLines.getAttribute("Name") +
        "," +
        devLines.getAttribute("LibType") +
        ",R0," +
        R[0] +
        ",Ohm\n";
      data_lines += ",,L0," + L[0] + ",Ohm\n";
      data_lines += ",,C0," + C[0] + "," + String.fromCharCode(181) + "F\n";
